<template>
  <name>CentOS-9</name>
  <os>
    <rootpw>ozrootpw</rootpw>
    <name>CentOS-9</name>
    %ifarch x86_64
    <version>4</version>
    <arch>x86_64</arch>
    %endif
    %ifarch s390x
    <version>9</version>
    <arch>s390x</arch>
    %endif  
    <install type="iso">
      <iso>file://<%= ISO_FILE.to_s %></iso>
    </install>
  </os>
  <description>CentOS Stream 9 s390x Image</description>
  <disk>
    <size><%= image_size %>G</size>
  </disk>
</template>
